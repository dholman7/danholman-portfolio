{
  "consumer": {
    "name": "user-service-consumer"
  },
  "provider": {
    "name": "user-service-provider"
  },
  "interactions": [
    {
      "description": "Get all users",
      "providerState": "users exist",
      "request": {
        "method": "GET",
        "path": "/api/users",
        "headers": {
          "Content-Type": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "users": [
            {
              "id": 1,
              "name": "John Doe",
              "email": "john@example.com",
              "active": true
            }
          ]
        },
        "matchingRules": {
          "$.body.users": {
            "min": 1
          },
          "$.body.users[*].*": {
            "match": "type"
          },
          "$.body.users[*].id": {
            "match": "type"
          },
          "$.body.users[*].name": {
            "match": "type"
          },
          "$.body.users[*].email": {
            "match": "type"
          },
          "$.body.users[*].active": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}